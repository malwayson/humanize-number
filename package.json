{
  "name": "@malwayson/humanize-number",
  "version": "2.0.0",
  "description": "Convert numbers to human readable format with support for 10 format methods including data, weight, distance, currency, temperature, duration, speed, volume, percentage and generic formats. Features metric/imperial unit systems, localization, presets, batch processing, and comparison utilities.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "example:basic": "npm run build && node dist/examples/basic-usage.js",
    "example:advanced": "npm run build && node dist/examples/advanced-features.js",
    "example:real-world": "npm run build && node dist/examples/real-world.js",
    "example:comprehensive": "npm run build && node dist/examples/comprehensive.js",
    "examples": "npm run build && node dist/examples/basic-usage.js && node dist/examples/advanced-features.js && node dist/examples/real-world.js"
  },
  "keywords": [
    "humanize",
    "number",
    "format",
    "formatter",
    "data",
    "weight",
    "distance",
    "currency",
    "temperature",
    "duration",
    "speed",
    "volume",
    "percentage",
    "readable",
    "human-readable",
    "convert",
    "conversion",
    "metric",
    "imperial",
    "unit-conversion",
    "units",
    "kg",
    "lbs",
    "pounds",
    "kilometers",
    "miles",
    "celsius",
    "fahrenheit",
    "localization",
    "i18n",
    "typescript",
    "batch-processing",
    "comparison",
    "presets"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md",
    "MIGRATION.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/malwayson/humanize-number.git"
  },
  "bugs": {
    "url": "https://github.com/malwayson/humanize-number/issues"
  },
  "homepage": "https://github.com/malwayson/humanize-number#readme"
}
